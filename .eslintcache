ame = props.slice( 6, props.length - 1).join(',');\n          }\n\n        } catch (e) {\n          item.props = \"error\"\n        }\n\n        try {\n          item.description = await PubChem.get(`/cid/${chem}/description/JSON`);\n          } catch (e) {\n            item.description = {\"data\": {\"InformationList\" : {\"Information\" : \"\"}}};\n          }\n        if (item.commonName == \"\") {\n\n        }\n        else {\n          chemData.push(item);\n        }\n\n\n        }\n        setRes(chemData);\n        hideLoader();\n\n    }\n    catch (e) {\n        hideLoader ();\n        setRes('error');\n        console.log('fug');\n      }\n\n\n  };\n  return [res, search, loader];\n\n};\n\n\n\n\nexport default useSearch;\n","/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/apis/rsc.js",[],["65","66"],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/components/ChemList.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/hooks/useLoadingPage.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/components/LoadingPage.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/components/NotFound.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/components/Chem3D.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/components/FormatError.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/apis/PubChem.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/components/Accordion.js",[],"/home/bbtvn/Modern React with Redux 2020/propios/chemistry/src/components/ChemItem.js",["67"],{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":13,"column":8,"nodeType":"74","endLine":13,"endColumn":30,"suggestions":"75"},{"ruleId":"76","severity":1,"message":"77","line":65,"column":29,"nodeType":"78","messageId":"79","endLine":65,"endColumn":31},{"ruleId":"68","repl